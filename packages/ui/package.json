{
  "name": "ui-kit",
  "version": "0.0.0",
  "description": "Ui kit",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "ui:dev": "turbo run package:dev --filter=ui-kit",
    "ui:build": "turbo run package:build --filter=ui-kit",
    "typescript": "vue-tsc --noEmit",
    "package:dev": "vite build",
    "package:build": "vue-tsc --p tsconfig.build.json && vite build"
  },
  "peerDependencies": {
    "vue": ">=3.5.0"
  },
  "devDependencies": {
    "@types/node": "20.19.27",
    "@vitejs/plugin-vue": "6.0.3",
    "glob": "13.0.0",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vue-tsc": "3.1.8"
  }
}
